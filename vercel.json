{
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "300mb",
          "includeFiles": [
            "models/**",
            "templates/**"
          ]
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "api/index.py"
      }
    ]
  }
  